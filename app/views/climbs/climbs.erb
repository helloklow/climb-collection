<h1>Welcome, <%= current_user.username %>!</h1>

<p><a href="/climbs/new">Add Climb</a>   |   <a href="/logout">Logout</a></p>

<h3><%= current_user.username %>'s Climbing Collection: </h3>

<br>

<% @climbs.each do |c| %>
  <p>
    Route: <%= c.name %>
    <br>
    Type: <%= c.type %>
    <br>
    Grade: <%= c.grade %>
    <br>
    Location: <%= c.location %>
    <br>
    Status: <%= c.status %>
    <br>
    Notes: <%= c.notes %>
    <br>
    <a href="/climbs/edit/<%= @climb.id %>">Edit Climb</a>
  </p>
<% end %>

<br><br>